<mapper namespace="com.rsh.easy_opm.UserMapper">

    <select id="selectByPrimaryKey" resultType="com.rsh.easy_opm.User">
        select
            id,
            nickName,
            realName,
            sex,
            age
        from user
        where id = ?
    </select>

    <select id="selectAll" resultType="com.rsh.easy_opm.User">
        select
            id,
            nickName,
            realName,
            sex,
            age
        from user
    </select>

</mapper>